{
    "name": "adinan-cenci/psr-17",
    "description": "An implementation of the PSR-17 specification.", 
    "type": "library", 
    "keywords": ["http", "psr", "psr-17"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AdinanCenci\\Psr17\\": "src/",
            "AdinanCenci\\Psr17\\Tests\\": "tests/"
        }
    },
    "require": {
        "adinan-cenci/psr-7": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev"
    },
    "scripts": {
        "unit-tests": [
            "./vendor/bin/phpunit tests"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adinan-cenci/psr-7.git"
        }
    ]
}
